<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.android.navigationadvancedsample.MainFragment"
        android:label="MainFragment" >
        <action
            android:id="@+id/action_mainFragment_to_registered2"
            app:destination="@id/registered2" />
    </fragment>
    <fragment
        android:id="@+id/registered2"
        android:name="com.example.android.navigationadvancedsample.formscreen.Registered"
        android:label="fragment_registered"
        tools:layout="@layout/fragment_registered" />
</navigation>